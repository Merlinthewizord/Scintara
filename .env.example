# Anthropic API
ANTHROPIC_API_KEY=
ANTHROPIC_MODEL=claude-3-5-sonnet-20241022

# OpenAI API (optional)
OPENAI_API_KEY=
MODEL_1=claude-opus-4-5-20251101
MODEL_2=claude-opus-4-5-20251101

# Dialogue schedule
DIALOGUE_EXCHANGES=12
DIALOGUE_INTERVAL_MINUTES=60
AUTO_ARCHIVE=true
CRON_SECRET=

# Supabase archive storage
SUPABASE_URL=
SUPABASE_SERVICE_ROLE_KEY=
SUPABASE_TABLE=conversations

# Archive storage
ARCHIVE_PATH=data\conversations.jsonl

# Generation defaults
MAX_NEW_TOKENS=256
TEMPERATURE=0.7
TOP_P=0.95

# API bind
HOST=0.0.0.0
PORT=8000
